<body>
    <div id="body">
<header id="hed1">
    <div id="idname">
        <p id="pname"><span>i</span>-NIMC <i class="fa fa-check"></i><span class="nimc_header">National Identity Management Commission</span></p>
        <span><i class="fa fa-bars" id="bars"></i></span>
    </div>
    <div class="search_cla">
      <input type="search" class="search_i" placeholder="search...">
      <label for="search"><i class="fa fa-search i_search"></i></label>
      <input type="submit" id="search">
    </div>
       <nav class ="nav active" id="nav_1">
           <a href="index" class = "a_nav">Home</a>
           <a href="register" class = "a_nav">Register</a>
